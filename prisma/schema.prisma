// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// [UPDATED] Model now matches the final table structure with all lowercase column names.
model maekok_summary_aggregated {
  hospcode      String
  hosname       String?
  provcode      String?
  provname      String?
  dist_name_th  String?
  date_serv     DateTime @db.Date
  diagcode      String
  groupname     String? // Changed from groupName
  total_count   BigInt?

  @@id([hospcode, date_serv, diagcode])
  @@map("maekok_summary_aggregated")
}

